<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.view.MainController">
   <children>
      <HBox maxHeight="200.0" maxWidth="600.0" minHeight="200.0" minWidth="600.0" prefHeight="200.0" prefWidth="600.0">
         <children>
            <VBox alignment="CENTER" maxHeight="200.0" maxWidth="300.0" minHeight="200.0" minWidth="300.0" prefHeight="200.0" prefWidth="300.0" spacing="10.0">
               <children>
                  <ComboBox fx:id="categories" onAction="#onCategorySelected" prefWidth="150.0" />
                  <ComboBox fx:id="products" onAction="#onProductSelected" prefWidth="150.0" />
               </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </VBox>
            <VBox alignment="CENTER" maxHeight="200.0" maxWidth="300.0" minHeight="200.0" minWidth="300.0" prefHeight="200.0" prefWidth="300.0" spacing="10.0">
               <children>
                  <Label text="Podaj cenÄ™ brutto" />
                  <TextField fx:id="nettoPrice" maxWidth="150.0" minWidth="150.0" prefWidth="150.0" />
               </children>
            </VBox>
         </children>
      </HBox>
      <TableView fx:id="table" prefHeight="200.0" prefWidth="200.0">
        <columns>
          <TableColumn fx:id="stateColumn" prefWidth="75.0" text="Stan" />
            <TableColumn fx:id="taxColumn" prefWidth="75.0" text="Podatek" />
          <TableColumn fx:id="priceColumn" prefWidth="97.0" text="Cena netto" />
        </columns>
      </TableView>
   </children>
</VBox>
